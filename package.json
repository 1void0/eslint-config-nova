{
  "name": "void0-utils",
  "version": "1.0.0",
  "description": "Some utilities to help you through your projects.",
  "private": true,
  "scripts": {
    "bootstrap": "lerna bootstrap --hoist",
    "build": "lerna run build",
    "commit": "git-cz",
    "lint": "lerna run lint",
    "release": "lerna run build && lerna publish",
    "test": "lerna run test-with-coverage"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "typescript",
    "react",
    "prettier",
    "import"
  ],
  "author": "CÃ©dric Trigoso",
  "repository": {
    "type": "git",
    "url": "https://github.com/1void0/utils.git"
  },
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "8.0.0",
    "@commitlint/config-conventional": "8.1.0",
    "@commitlint/config-lerna-scopes": "8.1.0",
    "@types/jest": "24.0.17",
    "@types/node": "12.7.1",
    "@typescript-eslint/eslint-plugin": "1.12.0",
    "@typescript-eslint/parser": "1.12.0",
    "commitizen": "4.0.3",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "6.1.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-find-rules": "3.3.1",
    "eslint-import-resolver-typescript": "1.1.1",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-jest": "22.9.0",
    "eslint-plugin-jsdoc": "15.8.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.2",
    "eslint-plugin-react-hooks": "1.6.1",
    "husky": "3.0.2",
    "jest": "24.8.0",
    "lerna": "3.16.4",
    "prettier": "1.18.2",
    "react": "16.8.6",
    "stylelint": "10.1.0",
    "ts-jest": "24.0.2",
    "typescript": "3.5.3"
  },
  "dependencies": {
    "@void0/commitlint-config": "file:packages/commitlint-config",
    "@void0/eslint-config": "file:packages/eslint-config",
    "@void0/eslint-config-react": "file:packages/eslint-config-react"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
