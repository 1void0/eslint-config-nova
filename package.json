{
  "name": "nova-utils",
  "version": "1.0.0",
  "description": "Some utilities to help you through your projects.",
  "private": true,
  "scripts": {
    "bootstrap": "lerna bootstrap --hoist",
    "commit": "git-cz",
    "lint": "lerna run lint",
    "release": "lerna publish",
    "test": "lerna run test"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "typescript",
    "react",
    "prettier",
    "import"
  ],
  "author": "CÃ©dric Trigoso",
  "repository": {
    "type": "git",
    "url": "https://github.com/1void0/nova-utils.git"
  },
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "7.6.1",
    "@commitlint/config-conventional": "7.6.0",
    "@commitlint/config-lerna-scopes": "7.6.0",
    "@types/jest": "24.0.12",
    "@types/node": "12.0.0",
    "@typescript-eslint/eslint-plugin": "1.7.0",
    "@typescript-eslint/parser": "1.7.0",
    "commitizen": "3.1.1",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.2.0",
    "eslint-find-rules": "3.3.1",
