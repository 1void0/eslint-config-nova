    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.13.0",
    "eslint-plugin-react-hooks": "1.6.0",
    "husky": "2.2.0",
    "jest": "24.8.0",
    "lerna": "^3.13.4",
    "prettier": "1.17.0",
    "react": "16.8.6",
    "ts-jest": "24.0.2",
    "typescript": "3.4.5"
  },
  "dependencies": {
    "eslint-config-nova": "file:packages/eslint-config-nova",
    "eslint-config-nova-react": "file:packages/eslint-config-nova-react"
  },
  "husky": {
    "hooks": {
