{
  "name": "void0-utils",
  "version": "1.0.0",
  "description": "Some utilities to help you through your projects.",
  "private": true,
  "scripts": {
    "bootstrap": "lerna bootstrap --hoist",
    "build": "lerna run build",
    "commit": "git-cz",
    "lint": "lerna run lint",
    "release": "lerna publish",
    "test": "lerna run test-with-coverage"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "typescript",
    "react",
    "prettier",
    "import"
  ],
  "author": "CÃ©dric Trigoso",
  "repository": {
    "type": "git",
    "url": "https://github.com/1void0/utils.git"
  },
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "8.0.0",
    "@commitlint/config-conventional": "8.0.0",
    "@commitlint/config-lerna-scopes": "8.0.0",
    "@types/jest": "24.0.15",
    "@types/node": "12.0.8",
    "@typescript-eslint/eslint-plugin": "1.10.2",
    "@typescript-eslint/parser": "1.10.2",
    "commitizen": "3.1.1",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "5.0.0",
    "eslint-find-rules": "3.3.1",
    "eslint-import-resolver-typescript": "1.1.1",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jest": "22.6.4",
    "eslint-plugin-jsdoc": "8.1.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.13.0",
    "eslint-plugin-react-hooks": "1.6.0",
    "husky": "2.4.1",
    "jest": "24.8.0",
    "lerna": "3.15.0",
    "prettier": "1.18.2",
    "react": "16.8.6",
    "ts-jest": "24.0.2",
    "typescript": "3.5.2"
  },
  "dependencies": {
    "@void0/commitlint-config": "file:packages/commitlint-config",
    "@void0/eslint-config": "file:packages/eslint-config",
