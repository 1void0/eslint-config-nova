{
  "name": "@void0/commitlint-config",
  "version": "1.0.3",
  "description": "Shareable commitlint config adding content and design to conventional commits",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint --ext .tsx --ext .ts --parser-options=tsconfigRootDir:./ --report-unused-disable-directives src/**",
    "test": "jest",
    "test-with-coverage": "jest --coverage"
  },
  "keywords": [
    "conventional-changelog",
    "commitlint",
    "commitlint-config",
    "conventions",
    "lint",
    "github",
    "labels"
  ],
  "author": "CÃ©dric Trigoso",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1void0/nova-utils.git",
    "directory": "packages/commitlint-config"
  },
  "bugs": {
    "url": "https://github.com/1void0/nova-utils/issues"
  },
  "homepage": "https://github.com/1void0/nova-utils#readme",
  "directories": {
    "doc": "docs"
  },
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@commitlint/config-conventional": "7.6.0"
  },
  "peerDependencies": {
    "@commitlint/cli": "8.0.0"
  }
}
