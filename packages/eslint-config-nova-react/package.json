{
  "name": "eslint-config-nova-react",
  "version": "1.0.6",
  "description": "Strict ESlint config for TypeScript and React projects using Prettier, JS modules and concerned about accessibility.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "eslint-find-deprecated-rules": "eslint-find-rules --deprecated",
    "eslint-find-unused-rules": "eslint-find-rules --unused",
    "lint": "eslint --ext .tsx --ext .ts --parser-options=tsconfigRootDir:./ --report-unused-disable-directives src/**",
    "test": "jest",
    "test-with-coverage": "jest --coverage"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "typescript",
    "react",
    "prettier",
    "import",
    "a11y",
    "react-hooks"
  ],
  "author": "CÃ©dric Trigoso",
  "repository": {
    "type": "git",
    "url": "https://github.com/1void0/nova-utils.git",
    "directory": "packages/eslint-config-nova-react"
  },
  "license": "ISC",
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": ">= 1.7.0",
    "@typescript-eslint/parser": ">= 1.7.0",
    "eslint": ">= 5.16.0",
    "eslint-config-nova": ">= 1.0.3",
    "eslint-config-prettier": ">= 4.3.x",
    "eslint-import-resolver-typescript": ">= 1.1.1",
    "eslint-plugin-import": ">= 2.17.3",
    "eslint-plugin-jest": ">= 22.5.1",
    "eslint-plugin-jsx-a11y": ">= 6.2.1",
    "eslint-plugin-prettier": ">= 3.0.1",
    "eslint-plugin-react": ">= 7.13.0",
    "eslint-plugin-react-hooks": ">= 1.6.0",
    "typescript": ">= 3.x"
  }
}
